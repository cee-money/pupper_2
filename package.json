    "axios": "^0.18.1",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "mysql2": "^1.6.5",
    "nodemailer": "^6.3.0",
